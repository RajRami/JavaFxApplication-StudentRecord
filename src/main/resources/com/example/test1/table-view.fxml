<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="450.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.test1.TableViewController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Students">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Label>
            <TableView fx:id="tableView" prefHeight="359.0" prefWidth="680.0">
              <columns>
                <TableColumn fx:id="studentId" prefWidth="75.0" text="Student#" />
                <TableColumn fx:id="firstName" prefWidth="75.0" text="First Name" />
                  <TableColumn fx:id="lastName" prefWidth="75.0" text="Last Name" />
                  <TableColumn fx:id="gender" prefWidth="75.0" text="Gender " />
                  <TableColumn fx:id="province" prefWidth="75.0" text="Province" />
                  <TableColumn fx:id="telephone" prefWidth="75.0" text="Telephone" />
                  <TableColumn fx:id="major" prefWidth="75.0" text="Major" />
                  <TableColumn fx:id="avgGrade" prefWidth="75.0" text="Avg Grade" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </TableView>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label text="Label">
                     <HBox.margin>
                        <Insets bottom="5.0" right="150.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox prefWidth="150.0" promptText="Select Province">
                     <HBox.margin>
                        <Insets left="150.0" />
                     </HBox.margin>
                  </ComboBox>
                  <Button mnemonicParsing="false" text="Button">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
